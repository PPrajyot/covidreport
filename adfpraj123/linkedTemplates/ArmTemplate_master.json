{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfpraj123"
		},
		"ls_blob_covidreport_praj_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_blob_covidreport_praj'"
		},
		"ls_gen2_covidreport_praj_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_gen2_covidreport_praj'"
		},
		"ls_casedeath_data_http_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com"
		},
		"ls_gen2_covidreport_praj_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://gen2covidpraj.dfs.core.windows.net/"
		},
		"trigger_ingest_covidreport_praj_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/32ef494a-beee-4985-8cbd-4ffd21de1c70/resourceGroups/praj_snow_pract/providers/Microsoft.Storage/storageAccounts/blobcovidpraj"
		},
		"trigger_ingest_hospitaladd_praj_properties_pl_ingest_ecdc_data_parameters_plpara_source_relativeURL": {
			"type": "string",
			"defaultValue": "cloudboxacademy/covid19/main/ecdc_data/hospital_admissions.csv"
		},
		"trigger_ingest_hospitaladd_praj_properties_pl_ingest_ecdc_data_parameters_plpara_sink_filename": {
			"type": "string",
			"defaultValue": "hospital_admission.csv"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfpraj123_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_blob_covidreport_praj_connectionString": {
						"value": "[parameters('ls_blob_covidreport_praj_connectionString')]"
					},
					"ls_gen2_covidreport_praj_accountKey": {
						"value": "[parameters('ls_gen2_covidreport_praj_accountKey')]"
					},
					"ls_casedeath_data_http_properties_typeProperties_url": {
						"value": "[parameters('ls_casedeath_data_http_properties_typeProperties_url')]"
					},
					"ls_gen2_covidreport_praj_properties_typeProperties_url": {
						"value": "[parameters('ls_gen2_covidreport_praj_properties_typeProperties_url')]"
					},
					"trigger_ingest_covidreport_praj_properties_typeProperties_scope": {
						"value": "[parameters('trigger_ingest_covidreport_praj_properties_typeProperties_scope')]"
					},
					"trigger_ingest_hospitaladd_praj_properties_pl_ingest_ecdc_data_parameters_plpara_source_relativeURL": {
						"value": "[parameters('trigger_ingest_hospitaladd_praj_properties_pl_ingest_ecdc_data_parameters_plpara_source_relativeURL')]"
					},
					"trigger_ingest_hospitaladd_praj_properties_pl_ingest_ecdc_data_parameters_plpara_sink_filename": {
						"value": "[parameters('trigger_ingest_hospitaladd_praj_properties_pl_ingest_ecdc_data_parameters_plpara_sink_filename')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}